<Window x:Class="System_decyzyjny.Backend"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:System_decyzyjny"
        mc:Ignorable="d"
        Title="Backend" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <DataGrid Grid.Row="0" x:Name="tabela" AutoGenerateColumns="True"/>
        <ListBox Grid.Row="1" x:Name="lista"/>
    </Grid>
</Window>
